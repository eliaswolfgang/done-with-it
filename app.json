{
  "expo": {
    "name": "done-with-it",
    "slug": "done-with-it",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "default",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "expo": {
      "scheme": "myapp",
      "web": {
        "bundler": "metro"
      }
    },
    "description": "A marketplace for selling and buying used items."
  }
}
